<Window x:Class="DB2FAtest.TwoFactorVerifyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DB2FAtest"
        mc:Ignorable="d"
        Title="TwoFactorVerifyWindow" Height="160.446" Width="297">
    <Grid>
        <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Margin="10,36,87,0"/>
        <Button x:Name="button" Content="Verify" HorizontalAlignment="Right" VerticalAlignment="Top" Width="75" Margin="0,39,7,0" Click="button_Click" IsDefault="True"/>
        <Label x:Name="label1" Content="Enter code generated by Google Authenticator" HorizontalAlignment="Left" Margin="10,10,0,0" Height="26" VerticalAlignment="Top"/>

    </Grid>
</Window>
